ce_per_unit_distance\":\"Price per km or mile for this service type.\",\n\t\"tooltip_distance_for_base_price\":\"Select the value for free distance covered in base price. Eg. if selected 2 then distance price will not be charged till 2 km or miles.\",\n\t\"tooltip_provider_profit\":\"Percentage of profit provider or driver should get for this service type.\",\n\t\"tooltip_admin_panel_name\":\"Admin panel name.\",\n\t\"tooltip_dispatcher_panel_name\":\"Dispatcher panel name.\",\n\t\"tooltip_partner_panel_name\":\"Partner panel name.\",\n\t\"tooltip_android_user_app_version_code\":\"Android User App Version.\",\n\t\"tooltip_android_provider_app_version_code\":\"Android Provider App Version.\",\n\t\"tooltip_ios_user_app_version_code\":\"Ios User App Version.\",\n\t\"tooltip_ios_provider_app_version_code\":\"Ios Provider App Version.\",\n\t\"tooltip_user_miscellaneous_fee\":\"Percentage of user miscellaneous fee on the total invoice.\",\n\t\"tooltip_user_tax\":\"Percentage of user tax on the total invoice.\",\n\t\"tooltip_provider_miscellaneous_fee\":\"Percentage of povider miscellaneous fee on the total invoice.\",\n\t\"tooltip_provider_tax\":\"Percentage of user tax on the total invoice.\",\n\t\"tooltip_road_api_google_key\":\"FOR ROAD API KEY\",\n\t\"tooltip_provider_offline_min\": \"Provider Offline after min\",\n\t\"camelot_header_decryption_stage1\": \"Header Encryption Stage 1 for CreateDriver API\",\n\t\"camelot_header_decryption_stage2\": \"Header Encryption Stage 2 for CreateDriver API\",\n\t\"camelot_header_auth_key\": \"Secret Authentication Key for CreateDriver API\",\n\t\"camelot_header_request_timeout\":\"Time in Seconds that should be allowed from current time for CreateDriver API\",\n\t\"camelot_provider_url\":\"Base URL for Driver Registration in Camelot System\",\n\t\"camelot_api_base_url\":\"Base URL for Camelot DPI API Requests\"\n}"